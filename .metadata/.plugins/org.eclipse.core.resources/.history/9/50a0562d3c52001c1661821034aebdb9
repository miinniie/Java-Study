import java.util.List;

public class SelectionTest2 {

	public SelectionTest2() {
		
		EmpDAO dao2 = new EmpDAO();
		List<EmpVO> list = dao.allEmpSelect();
		
		for(EmpVO vo: list) {//확장된 for문으로 돌리기
			if(vo.getEmpno()==1234)
			System.out.printf("%7d\n %10s\n %10s\n %7.2f\n %7.2f\n %s\n",
					vo.getEmpno(), vo.getEname(), vo.getJob(), 
					vo.getSal(), vo.getComm(), vo.getHiredate());
		}
		
		
		/*
		 실행결과
		 사원번호: 1234
		 사원명 -> ___
		 담당업무->___
		 급여->____
		 보너스_>____
		 */
	}

	public static void main(String[] args) {
		new SelectionTest2();
	}

}
